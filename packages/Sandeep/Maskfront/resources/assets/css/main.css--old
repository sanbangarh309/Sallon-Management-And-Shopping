  -ms-user-select: none;
  user-select: none;
  margin-top: 10px;
}


.custom-check input {
	position: absolute;
	opacity: 0;
	cursor: pointer;
	float: left;
	width: 100%;
	height: 25px;
	left: 0;
	z-index: 9;
}

.custom-check label {
	position: relative;
	top: 0;
	left: 40px;
	height: 25px;
	width: 100%;
	font-weight: 400;
	font-size: 17px;
	line-height: 19px;
}
.custom-check label span {
	font-size: 75%;
	display: block;
	margin-top: 0;
}

.custom-check input + label::before {
  background-color: #000000;
  height:20px;
  width:20px;
  content:"";
  border:solid 1px #ccc;
  position:absolute;
  left:-35px;
  top:3px;
}


.custom-check input + label::after {
	left: -24px;
	top: -3px;
	width: 12px;
	height: 20px;
	border: solid #edba71;
	border-top-width: medium;
	border-right-width: medium;
	border-bottom-width: medium;
	border-left-width: medium;
	border-width: 0 3px 3px 0;
	-webkit-transform: scale(1.9) rotate(0);
	-ms-transform:scale(1.9) rotate(0);
	transform:scale(1.9) rotate(0);
	position: absolute;
	transition:all 0.3s ease 0s;
	content:"";
	opacity:0;
	
	
}

.custom-check input:checked + label::after, .custom-check input:hover + label::after{
	-webkit-transform: scale(1) rotate(45deg);
	-ms-transform:scale(1) rotate(45deg);
	transform:scale(1) rotate(45deg);
	opacity:1;
}
