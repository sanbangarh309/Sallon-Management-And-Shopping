.cstm--checkbox label{font-size:16px;}
.cstm--checkbox input[type="checkbox"] + label::before {background: #fff ;border: 2px solid #b8b8b8;content: "";height: 20px;left:14px;position: absolute;top: 13px;width: 20px;transition: all 0.2s ease 0s;z-index:-1;}
.cstm--checkbox > [type="checkbox"]:not(:checked), .cstm--checkbox > [type="checkbox"]:checked {left:15px;position: absolute;z-index:99999;opacity:0;width:20px;cursor: pointer;top:0;}
.cstm--checkbox input[type="checkbox"]:checked + label::before {background: #000;border: 2px solid #000;}
.cstm--checkbox > [type="checkbox"] + label::after {color: #fff;content: "?";font: 15px/1 FontAwesome;left:18px;margin-top: -4px;position: absolute;
top: 50%;opacity: 0;transform: scale(3) rotateZ(-20deg);transition: all 0.3s ease-in 0s;}
.cstm--checkbox > [type="checkbox"]:checked + label::after {opacity:1;transform: scale(1) rotateZ(0);}
