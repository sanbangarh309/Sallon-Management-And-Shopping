/*************************
*******Typography******
**************************/

body {font-family: 'Lato', sans-serif;font-size: 14px;line-height: 24px;color: #666;background-color: #000;}
h1, h2, h3, h4, h5, h6 {color: #333;}
h2 {font-size: 30px;margin-bottom: 20px;}
h3 {font-size: 18px;}
.btn {border: 0;border-radius: 0;opacity:0.8;transition:all 0.3s ease 0s;}
.btn:hover {opacity:1;}
.navbar-nav li a:hover, .navbar-nav li a:focus {outline:none;outline-offset: 0;text-decoration:none; background: transparent;}
a {text-decoration: none;-webkit-transition: 300ms;-moz-transition: 300ms;-o-transition: 300ms;transition: 300ms;}
a:focus, a:hover {text-decoration: none;outline: none}
section {padding: 90px 0;}
.heading {padding-bottom:90px;}
.preloader {position: fixed;top: 0;left: 0;right: 0;bottom: 0;z-index: 999999;background: #fff;}
.preloader > i {position: absolute;font-size: 36px;line-height: 36px;top: 50%;left: 50%;height: 36px;width: 36px;margin-top: -15px;margin-left: -15px;
display: inline-block;color:#d68311;}
